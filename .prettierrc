{
  "useTabs": false,
  "semi": true,
  "singleQuote": false,
  "arrowParens": "always",
  "endOfLine": "crlf",
  "tabWidth": 2,
  "svelteIndentScriptAndStyle": false,
  "trailingComma": "none",
  "printWidth": 100,
  "plugins": ["prettier-plugin-svelte"],
  "pluginSearchDirs": ["."],
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
